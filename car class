class Car 
{
    String model;
    int year;
    Car(String model, int year)
    {
        this.model = model;
        this.year = year;
    }
    void display() 
    {
        System.out.println("Model: " + model);
        System.out.println("Year: " + year);
    }
}
public class Main
{
    public static void main(String[] args) 
    {
        Car car1 = new Car("Toyota Camry", 2020);
        car1.display();
    }
}
