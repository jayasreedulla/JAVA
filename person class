class Person
{
    String name;
    int age;
    void display() 
    {
        System.out.println("Name: " + name);
        System.out.println("Age: " + age);
    }
}
public class Main
{
    public static void main(String[] args)
    {
        Person person1 = new Person();
        person1.name = "John";
        person1.age = 25;
        person1.display();
    }
}
