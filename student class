class Student {
    String name;
    int rollNumber;
    double marks;

    void display() {
        System.out.println("Name: " + name);
        System.out.println("Roll Number: " + rollNumber);
        System.out.println("Marks: " + marks);
    }

    public static void main(String[] args) {
        Student student = new Student();
        student.name = "Alice";
        student.rollNumber = 101;
        student.marks = 95.5;
        student.display();
    }
}
